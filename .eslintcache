ercises={activeMuscle.excersises} isMobile={mobile} />\n      )}\n    </MuscleGroupsWrapper>\n  );\n};\n\nexport default App;\n","C:\\Users\\thoma\\source\\repos\\ndla-muskelgrupper\\src\\utils\\useIsMobile.js",[],"C:\\Users\\thoma\\source\\repos\\ndla-muskelgrupper\\src\\utils\\useFullscreen.js",[],{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","replacedBy":"53"},{"ruleId":"50","replacedBy":"54"},{"ruleId":"52","replacedBy":"55"},{"ruleId":"56","severity":1,"message":"57","line":101,"column":10,"nodeType":"58","messageId":"59","endLine":101,"endColumn":22},"no-native-reassign",["60"],"no-negated-in-lhs",["61"],["60"],["61"],"no-unused-vars","'isFullscreen' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]